{
  "name": "node-play-ui",
  "version": "0.0.1",
  "description": "The node-play ui using vue.js",
  "author": "Alessandro <aseduto@hotmail.com>",
  "private": true,
  "scripts": {
    "old-unit": "mocha-webpack --webpack-config webpack.config.js --require test/unit/setup.js --mode development \"test/unit/**/*.spec.js\"",
    "unit-build": "webpack --config ./webpack.config.js --entry ./test/unit/components/operation.spec.js  --output ./bin/unittest.js --mode development --target node"
    , "unit": "mocha --require test/unit/setup.js ./bin/unittest.js"
    , "unit-cov": "nyc npm run unit"
    , "cov-html": "nyc report --reporter=html"
    , "lint": "eslint --ext .js,.vue src ",
    "build-dev": "webpack --mode development",
    "build": "webpack --mode production",
    "serve": "webpack-dev-server --open --hot",
    "start": "node node_modules/@mediagoom/devman",
    "deploy-clean-dir": "shx --silent rm -fr ../node-play/dist",
    "deploy-mkdir-dir": "shx mkdir ../node-play/dist",
    "deploy-mkdir-script": "shx mkdir ../node-play/dist/script",
    "deploy-copy-html": "shx cp index.html ../node-play/dist",
    "deploy-copy-ico": "shx cp *.png ../node-play/dist",
    "deploy-copy-asset": "shx cp ./bin/*.* ../node-play/dist/script",
    "deploy": "run-s build deploy-clean-dir deploy-mkdir-dir deploy-mkdir-script deploy-copy-html deploy-copy-ico deploy-copy-asset"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@mediagoom/chunk-upload": "0.0.19",
    "@mediagoom/play": "0.0.5",
    "axios": "^0.19.0",
    "debug": "^4.1.0",
    "vue-resource": "^1.2.1",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.4.2",
    "@mediagoom/devman": "0.0.12",
    "@vue/test-utils": "^1.0.0-beta.26",
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "postcss-inline-svg": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "2.0.1",
    "shx": "^0.3.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
