{
  "name": "node-play-ui",
  "version": "0.0.1",
  "description": "The node-play ui using vue.js",
  "author": "Alessandro <aseduto@hotmail.com>",
  "private": true,
  "scripts": {
    "unit": "mocha-webpack --webpack-config webpack.config.js --require test/unit/setup.js --mode development \"test/unit/**/*.spec.js\"",
    
    "lint": "eslint --ext .js,.vue src ",
    "build-dev": "webpack --mode development",
    "build": "webpack --mode production",
    "serve": "webpack-dev-server --open --hot",
    "start": "node node_modules/@mediagoom/devman"

    , "deploy-clean-dir": "shx --silent rm -fr ../node-play/dist"
    , "deploy-mkdir-dir": "shx mkdir ../node-play/dist"
    , "deploy-mkdir-script": "shx mkdir ../node-play/dist/script"
    , "deploy-copy-html": "shx cp index.html ../node-play/dist"
    , "deploy-copy-asset": "shx cp ./bin/*.* ../node-play/dist/script"

    , "deploy" : "run-s deploy-clean-dir deploy-mkdir-dir deploy-mkdir-script deploy-copy-html deploy-copy-asset"
  },
  "dependencies": {
    "@mediagoom/chunk-upload": "0.0.10",
    "@mediagoom/play": "0.0.5",
    "axios": "^0.15.3",
    "debug": "^4.1.0",
    "vue": "^2.2.1",
    "vue-resource": "^1.2.1",
    "vue-router": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.0.0",
    "@mediagoom/devman": "^0.0.8",
    "@vue/test-utils": "^1.0.0-beta.26",
    "autoprefixer": "^6.7.2",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "2.0.0",
    "shx": "^0.3.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
